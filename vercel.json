{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/user/(.*)", "dest": "index.js" },
    { "src": "/getblogs/(.*)", "dest": "index.js" },
    { "src": "/gtcomments/(.*)", "dest": "index.js" },
    { "src": "/(.*)", "dest": "index.js" }
  ],
 
}
